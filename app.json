{
  "expo": {
    "name": "Rork-FineDine",
    "slug": "rork-finedine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rorkfinedine",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F97316"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.rorkfinedine.app",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to upload restaurant and menu photos.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show nearby restaurants.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photos to upload images."
      }
    },
    "android": {
      "package": "com.rorkfinedine.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F97316"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "VIBRATE",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": false
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#F97316"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiUrl": "http://localhost:8080/api/v1",
      "apiUrlProduction": "https://primedine.fly.dev/",
      "supabaseUrl": "https://qtmvztgxfvqmqekdhnxw.supabase.co",
      "supabaseAnonKey": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6InF0bXZ6dGd4ZnZxbXFla2RobnhXIiwicm9sZSI6ImFub24iLCJpYXQiOjE3MDkxMjM0NTYsImV4cCI6MjAyNDY5OTQ1Nn0.xxxxx",
      "eas": {
        "projectId": "your-eas-project-id"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/your-eas-project-id"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
