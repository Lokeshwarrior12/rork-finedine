PORT=8080
ENV=development

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_JWT_SECRET=your-jwt-secret

# Redis (optional - for caching)
REDIS_URL=redis://localhost:6379
# Or for Upstash:
# REDIS_REST_URL=https://your-upstash-redis.upstash.io
# REDIS_REST_TOKEN=your-rest-token
flyctl secrets set SUPABASE_URL=your_supabase_url_here
flyctl secrets set SUPABASE_ANON_KEY=your_anon_key_here
flyctl secrets set JWT_SECRET=your_jwt_secret_here
flyctl secrets set STRIPE_SECRET_KEY=sk_test_...
flyctl secrets set REDIS_URL=redis://default:yourpassword@your-upstash-host:6379
